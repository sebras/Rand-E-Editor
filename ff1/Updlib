set v = ""
if $1x != -vx goto lib
set v = v
shift
goto lib
:lib
if $1x = ffx   goto doit
if $1x = ffdx goto doit
    echo example: $0 ff
    exit
: doit
cd o$1
nar $vu ../lib$1.a
close.o \
fd.o    \
flush.o \
free.o  \
getc.o  \
open.o  \
pos.o   \
putc.o  \
read.o  \
seek.o  \
size.o  \
write.o \
alloc.o \
blk.o   \
sort.o

